.setcomp {
  background-color: #eee;
  padding: 20rpx;
}

.set-input {
  height: 60rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5px;
}

.srcimages {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 20rpx 0;
}

.set-input icon {
  margin-left: 5px;
}

.hander-top {
  width: 750rpx;
  height: 100rpx;
  border-bottom: 1px solid #ccc;
  display: flex;
  text-align: center;
  align-items: center;
}

.hander-top view {
  width: 33.3%;
}

.hander-top view:nth-child(3) {
  background-color: red;
}

.content-list {
  width: 750rpx;
  height: 260rpx;
  margin-left: 21rpx;
}

.content-list .content-son {
  width: 100%;
  height: 100%;
  border-bottom: 1px solid #ccc;
}

.content-son .left {
  width: 200rpx;
  height: 100%;
  display: flex;
  text-align: center;
  align-items: center;
}

.content-son .left image {
  width: 200rpx;
  height: 200rpx;
}

.content-son {
  display: flex;
}

.content-son .right {
  margin-left: 20px;
  margin-top: 20px;
  position: relative;
}
.content-son .right text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.content-son .right text + text{
  background-color: red;
  position: absolute;
  bottom: 20px;
  left: 0;
}
navigator{
display: block;
}
.active{
  color: red;
}
